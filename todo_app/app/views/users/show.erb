<h1><%= @user.username %></h1>

<%= link_to "Create new list", new_list_path %>

<% @lists.each do |list| %>
  <div class='list'>
    <%= link_to list.name, list_path(list)%><br>
    <%= link_to "Edit", edit_list_path(list) %>
    <%= link_to "Delete", list, method: :delete %>
  </div>
<% end %>

<%= link_to "Home", :root %>

